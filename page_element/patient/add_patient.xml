<elements>
    <!--  新增患者对话框  -->
    <element>
        <!--  新增患者标题  -->
        <name>AddPatientTitle</name>
        <type>css</type>
        <value>div.large > div.el-dialog__header</value>
    </element>
    <element>
        <!--  关闭按钮  -->
        <name>CloseBtn</name>
        <type>css</type>
        <value>div.large > div.el-dialog__header > button.el-dialog__headerbtn</value>
