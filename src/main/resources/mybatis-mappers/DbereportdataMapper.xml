<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zw.admin.server.dao.DbereportdataDao">

	<sql id="where">
		<where>
			<if test="params.aromarea != null and params.aromarea != ''">
				and aromarea = #{params.aromarea} 
			</if>
			<if test="params.aromfourpoints != null and params.aromfourpoints != ''">
				and aromfourpoints = #{params.aromfourpoints} 
			</if>
			<if test="params.arompoints != null and params.arompoints != ''">
				and arompoints = #{params.arompoints} 
			</if>
			<if test="params.createtime != null and params.createtime != ''">
				and createtime = #{params.createtime} 
			</if>
			<if test="params.id != null and params.id != ''">
				and id = #{params.id} 
			</if>
			<if test="params.image != null and params.image != ''">
				and image = #{params.image} 
			</if>
			<if test="params.promarea != null and params.promarea != ''">
				and promarea = #{params.promarea} 
			</if>
			<if test="params.promfourpoints != null and params.promfourpoints != ''">
				and promfourpoints = #{params.promfourpoints} 
			</if>
			<if test="params.prompoints != null and params.prompoints != ''">
				and prompoints = #{params.prompoints} 
			</if>
			<if test="params.remark != null and params.remark != ''">
				and remark = #{params.remark} 
			</if>
			
			<if test="params.type != null and params.type != ''">
				and type = #{params.type} 
			</if>
			<if test="params.userId != null and params.userId != ''">
				and user_id = #{params.userId} 
			</if>
			<if test="params.xaromvalue != null and params.xaromvalue != ''">
				and xaromvalue = #{params.xaromvalue} 
			</if>
			<if test="params.xpromvalue != null and params.xpromvalue != ''">
				and xpromvalue = #{params.xpromvalue} 
			</if>
			<if test="params.yaromvalue != null and params.yaromvalue != ''">
				and yaromvalue = #{params.yaromvalue} 
			</if>
			<if test="params.ypromvalue != null and params.ypromvalue != ''">
				and ypromvalue = #{params.ypromvalue} 
			</if>
		
		</where>
	</sql>

	<select id="count" resultType="int">
		select count(1) from dbereportdata t
		<include refid="where" />
	</select>

	<select id="list" resultType="Dbereportdata">
		select * from dbereportdata t
		<include refid="where" />
		order by id desc
		<if test="limit != null and limit != 0">
		limit #{offset}, #{limit}
		</if>
	</select>

	<update id="update">
		update dbereportdata t
		<set>
			<if test="aromarea != null">
				aromarea = #{aromarea}, 
			</if>
			<if test="aromfourpoints != null">
				aromfourpoints = #{aromfourpoints}, 
			</if>
			<if test="arompoints != null">
				arompoints = #{arompoints}, 
			</if>
			<if test="createtime != null">
				createtime = #{createtime}, 
			</if>
			<if test="image != null">
				image = #{image}, 
			</if>
			<if test="promarea != null">
				promarea = #{promarea}, 
			</if>
			<if test="promfourpoints != null">
				promfourpoints = #{promfourpoints}, 
			</if>
			<if test="prompoints != null">
				prompoints = #{prompoints}, 
			</if>
			<if test="remark != null">
				remark = #{remark}, 
			</if>
			
			<if test="type != null">
				type = #{type}, 
			</if>
			<if test="user_id != null">
				user_id = #{userId}, 
			</if>
			<if test="xaromvalue != null">
				xaromvalue = #{xaromvalue}, 
			</if>
			<if test="xpromvalue != null">
				xpromvalue = #{xpromvalue}, 
			</if>
			<if test="yaromvalue != null">
				yaromvalue = #{yaromvalue}, 
			</if>
			<if test="ypromvalue != null">
				ypromvalue = #{ypromvalue}, 
			</if>

		</set>

		where t.id = #{id}
	</update>

</mapper>
